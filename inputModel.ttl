@prefix model: <http://uni-ko-ld.de/ist/model#> .
@prefix prod: <http://uni-ko-ld.de/ist/product#> .
@prefix proc: <http://uni-ko-ld.de/ist/process#> .
@prefix res: <http://uni-ko-ld.de/ist/resource#> .
@prefix properties: <http://uni-ko-ld.de/ist/resource#> .

#/* 1. Process */
proc:senorFlashing model:hasInputProduct prod:radarSensor.
proc:senorFlashing model:hasResource, res:firmwareFlasher. 
proc:senorFlashing model:hasMeassurement, prop:firmwareVersion.
proc:senorFlashing model:hasConstraint, prop:firmwareVersion.
proc:senorFlashing model: hasOutputProduct, prod:flashedRadarSensor.

#/* Link between 1. Process and 2. Process*/
proc:sensorFlashing model:hasNextProcess proc:caseAssembly.

#/* 2. Process */
proc:caseAssembly model:hasInputProduct prod:flashedRadarSensor.
proc:caseAssembly model:hasResource, res:placer.
proc:caseAssembly model:hasResource, res:screwer.
proc:caseAssembly model:hasMeassurement, prop:position.
proc:caseAssembly model:hasMeassurement, prop:force.
proc:caseAssembly model:hasConstraint, prop:position.
proc:caseAssembly model:hasConstraint, prop:force.
proc:caseAssembly model:hasOutputProduct, prod:assembledSensor.
